<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="editar.png" />
    <None Remove="imgs\editar.png" />
    <None Remove="imgs\eliminar.png" />
    <None Remove="imgs\guardar.png" />
    <None Remove="imgs\nuevo.png" />
    <None Remove="nuevo.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Data.SqlClient" Version="4.8.6" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="imgs\editar.png" />
    <Resource Include="imgs\eliminar.png">
      <CopyToOutputDirectory></CopyToOutputDirectory>
    </Resource>
    <Resource Include="imgs\guardar.png">
      <CopyToOutputDirectory></CopyToOutputDirectory>
    </Resource>
    <Resource Include="imgs\nuevo.png" />
  </ItemGroup>

</Project>
